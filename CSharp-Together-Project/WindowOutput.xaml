<Window x:Class="CSharp_Together_Project.WindowOutput"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CSharp_Together_Project"
        mc:Ignorable="d"
        Title="WindowOutput" Height="450" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition />
        </Grid.ColumnDefinitions>

        <Grid.RowDefinitions>
            <RowDefinition Height="*" />
            <RowDefinition Height="350" />
        </Grid.RowDefinitions>

        <TextBox x:Name="search_input" Grid.Row="0" Grid.Column="0" Margin="0,0,218,0"></TextBox>
        <WebBrowser x:Name="search_output" Margin="10,22,10,21" Grid.Row="1" />
        <Button Content="Searh" FontSize="25" HorizontalAlignment="Left" Margin="562,0,0,0" VerticalAlignment="Center" Height="84" Width="238" Click="SearchData"/>
    </Grid>
</Window>
